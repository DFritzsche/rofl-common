#ifndef __ROFL_CONFIG_H__
#define __ROFL_CONFIG_H__

/* Name of package */
#define ROFL_PACKAGE "@PACKAGE@"

/* Define to the address where bug reports for this package should be sent. */
#define ROFL_PACKAGE_BUGREPORT "@PACKAGE_BUGREPORT@"

/* Define to the full name of this package. */
#define ROFL_PACKAGE_NAME "@PACKAGE_NAME@"

/* Define to the full name and version of this package. */
#define ROFL_PACKAGE_STRING "@PACKAGE_STRING@"

/* Define to the one symbol short name of this package. */
#define ROFL_PACKAGE_TARNAME "@PACKAGE_TARNAME@"

/* Define to the home page for this package. */
#define ROFL_PACKAGE_URL "@PACKAGE_URL@"

/* Define to the version of this package. */
#define ROFL_PACKAGE_VERSION "@PACKAGE_VERSION@"

/* Compiled in branch(local) */
#define ROFL_BRANCH "@ROFL_BRANCH@"

/* Build (git hash) */
#define ROFL_BUILD "@ROFL_BUILD@"

/* Git describe */
#define ROFL_DESCRIBE "@ROFL_DESCRIBE@"

/* ROFL version */
#define ROFL_VERSION_ "@ROFL_VERSION@"

/* Have OPENSSL */
@ROFL_HAVE_OPENSSL@

/* Compiled with pipeline support */
@ROFL_PIPELINE_PRESENT@

/* byteswap.h */
@BYTESWAP_HEADER_DETECTED@

/* endianness */
@BIG_ENDIAN_DETECTED@
@LITTLE_ENDIAN_DETECTED@

/* Compiled with inline PP API */
@ROFL_PIPELINE_INLINE_PP_PLATFORM_FUNCS@

/* MACRO that if used prevents pipeline to be used with PP inlined */
@ROFL_PIPELINE_ABORT_IF_INLINED@

/* pipeline max tids */
@ROFL_PIPELINE_MAX_TIDS@

/* pipeline locked tid */
@ROFL_PIPELINE_LOCKED_TID@

/* pipeline lockless */
@ROFL_PIPELINE_LOCKLESS@

#endif //__ROFL_CONFIG_H__
